datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Transfer {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())

  from      String
  to        String
  amount    String
  lamports  String
  signature String
  block     String
}
